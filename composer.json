{
    "name": "bytesystems/settings-bundle",
    "description": "A settings manager with doctrine storage",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Heim",
            "email": "heim@bytesystems.de"
        }
    ],
    "require": {
        "php": "^7.0|^8.0",
        "doctrine/orm": "^2.6",
        "doctrine/doctrine-bundle": "^2.3",
        "symfony/config": "^4.4 | ^5.0 | ^6.0",
        "symfony/dependency-injection": "^4.4 | ^5.0 | ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Bytesystems\\SettingsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bytesystems\\SettingsBundle\\Tests\\": "tests/"
        }
    }
}
