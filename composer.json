{
    "name": "bytesystems/settings-bundle",
    "description": "A settings manager with doctrine storage",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Heim",
            "email": "heim@bytesystems.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/orm": "^2.0",
        "symfony/config": "^6.0",
        "symfony/dependency-injection": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Bytesystems\\SettingsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bytesystems\\SettingsBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "doctrine/doctrine-bundle": "^2.0",
        "symfony/yaml": "^6.0",
        "liip/test-fixtures-bundle": "^2.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0",
        "rector/rector": "^0.18.13"
    }
}
