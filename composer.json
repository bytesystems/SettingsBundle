{
    "name": "bytesystems/settings-bundle",
    "description": "A settings manager with doctrine storage",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Heim",
            "email": "heim@bytesystems.de"
        }
    ],
    "require": {
        "php": "^8.0",
        "doctrine/orm": "^2.6",
        "symfony/config": "^4.4 | ^5.0 | ^6.0",
        "symfony/dependency-injection": "^4.4 | ^5.0 | ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Bytesystems\\SettingsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bytesystems\\SettingsBundle\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^6.1",
        "symfony/http-kernel": "^4.4 | ^5.0 | ^6.0",
        "symfony/framework-bundle": "^4.4 | ^5.0 | ^6.0",
        "doctrine/doctrine-bundle": "^2.3",
        "symfony/yaml": "^4.4 | ^5.0 | ^6.0",
        "liip/test-fixtures-bundle": "^2.4",
        "doctrine/doctrine-fixtures-bundle": "^3.4"
    }
}
